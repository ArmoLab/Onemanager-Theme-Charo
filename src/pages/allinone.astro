---
import "../assets/base.css"


// base Components
import Divider from "../components/base/Divider.astro"

// container
import ContentContainer from "../components/base/ContentContainer.astro"
import LinkContainer from "../components/base/LinkContainer.astro"



import Nav from "../components/NavBar.astro"
import PathBar from "../components/PathBar.astro"
import MorePage from "components/MorePage.astro"


---

<!--IconValuesStart-->

<!--

{
    "music":"musical-notes",
    "video":"logo-youtube",
    "img":"image",
    "office":"newspaper",
    "txt":"clipboard",
    "zip":"file-tray-full",
    "iso":"disc",
    "apk":"logo-android",
    "exe":"logo-windows",
    "default":"document"
}

-->

<!--IconValuesEnd-->


<html lang="<!--constStr@language-->">
	<head>
		<meta charset="utf-8" />
        <meta name="viewport" content="width=device-width,initial-scale=1" />
        <meta name="keywords" content="<!--Keywords-->" />
        <meta name="description" content="<!--Description-->" />
        <link rel="icon" href="<!--base_path-->favicon.ico" type="image/x-icon" />
        <link rel="shortcut icon" href="<!--base_path-->favicon.ico" type="image/x-icon" />
        <title><!--Title--></title>


        <!--MdRequireStart-->
            <link rel="stylesheet" href="/aom-assets/depends/marked/github-markdown.css">
            <script type="text/javascript" src="/aom-assets/depends/marked/marked.js"></script>
        <!--MdRequireEnd-->

        <!--customCss-->
	</head>
	<body>
		<Nav SiteName="<!--Sitename-->" />
		<main>

		    <!--HeadomfStart-->
		        <ContentContainer>
                    <div class="customfile" id="head-om">
                        <!--HeadomfContent-->
                    </div>
		        </ContentContainer>
            <!--HeadomfEnd-->
            <!--HeadmdStart-->
                <ContentContainer>
                    <div class="markdown-body" id="head">
                        <textarea id="head-md" style="display:none;"><!--HeadmdContent--></textarea>
                    </div>
                </ContentContainer>
            <!--HeadmdEnd-->



		    <ContentContainer>
            <div>

                <PathBar />

                <Divider />
                <h3><!--constStr@File--></h3>

                

                <!--IsFolderStart-->

                <div style="display: flex; justify-content: space-between;margin: 0 5%;padding: 0 10px" id= "<!--FolderId-->">
                    <div style="width: 60%;text-overflow: ellipsis;overflow: hidden;white-space: nowrap;"><!--constStr@File--></div>
                    <div style="width: 20%;text-align: right;"><!--constStr@EditTime--></div>
                    <div style="width: 20%;text-align: right;"><!--constStr@Size--></div>
                </div>



                    <!--FolderListStart-->
                    <LinkContainer href="<!--FileEncodeReplaceUrl-->/">
                        <div style="display: flex; justify-content: space-between;">
                            <div
                                style="width: 50%;text-overflow: ellipsis;overflow: hidden;white-space: nowrap;"
                                data-FileId="<!--FileId-->"
                            >
                                <!--FileEncodeReplaceName-->
                            </div>
                            <div style="width: 30%;text-align: right;">
                                <!--lastModifiedDateTime-->
                            </div>
                            <div style="width: 20%;text-align: right;">
                                <!--size-->
                            </div>
                        </div>
                    </LinkContainer>
                    <!--FolderListEnd-->



                    <!--FileListStart-->
                    <LinkContainer href="<!--FileEncodeReplaceUrl-->?preview">
                        <div style="display: flex; justify-content: space-between;">
                            <div
                            style="width: 50%;text-overflow: ellipsis;overflow: hidden;white-space: nowrap;"
                                data-FileId="<!--FileId-->"
                            >
                                <!--FileEncodeReplaceName-->
                            </div>
                            <div
                                style="width: 30%;text-align: right;"
                            >
                                <!--lastModifiedDateTime-->
                            </div>
                            <div
                                style="width: 20%;text-align: right;"
                            >
                                <!--size-->
                            </div>
                        </div>
                    </LinkContainer>
                    <!--FileListEnd-->

                    <MorePage />

                <!--IsFolderEnd-->

                </div>

		    </ContentContainer>
            


            <!--ReadmemdStart-->
		        <ContentContainer>
                    <div class="markdown-body" id="readme">
                        <textarea id="readme-md" style="display:none;"><!--ReadmemdContent--></textarea>
                    </div>
		        </ContentContainer>
            <!--ReadmemdEnd-->
            <!--FootomfStart-->
		        <ContentContainer>
                    <div class="customfile" id="foot-om">
                        <!--FootomfContent-->
                    </div>
		        </ContentContainer>
            <!--FootomfEnd-->

		</main>
	</body>
	<!--customScript-->
</html>
